{
  "mcpServers": {
    "aws-kb": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "fastmcp",
        "proxy",
        "https://knowledge-mcp.global.api.aws"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "timeout": 100000,
      "disabled": false,
      "autoApprove": []
    },
    "aws-frontend": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "awslabs.frontend-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "aws-cdk": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "awslabs.cdk-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "fetch": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "mcp-server-fetch@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}
