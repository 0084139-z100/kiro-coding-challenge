{
  "mcpServers": {
    "aws-kb": {
      "command": "fastmcp",
      "args": ["proxy", "https://knowledge-mcp.global.api.aws"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "aws-frontend": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "--from",
        "awslabs.frontend-mcp-server@latest",
        "awslabs.frontend-mcp-server"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "aws-cdk": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "--from",
        "mcp-server-aws-cdk@latest",
        "mcp-server-aws-cdk"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "fetch": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "--from",
        "mcp-server-fetch@latest",
        "mcp-server-fetch"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}
